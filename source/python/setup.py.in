  sys.exit('Python version must be 3 or higher.')

setup(
  name        = 'pyromocc',
  version     = '${VERSION}',
  packages    = ['pyromocc'],
  package_dir = {'': '${PROJECT_BINARY_DIR}'},
  package_data = {'': ['pyromocc.*.so']}
)